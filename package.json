{
  "name": "tickplate",
  "version": "0.0.6",
  "author": "Timur Shemsedinov <timur.shemsedinov@gmail.com>",
  "description": "Back-tick templates for JavaScript",
  "license": "MIT",
  "keywords": [
    "tickplate",
    "metarhia",
    "tick",
    "backtick",
    "template",
    "templating"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/metarhia/tickplate"
  },
  "bugs": {
    "url": "https://github.com/metarhia/tickplate/issues",
    "email": "timur.shemsedinov@gmail.com"
  },
  "main": "./tickplate.js",
  "engines": {
    "node": ">=8.0.0"
  },
  "readmeFilename": "README.md",
  "scripts": {
    "test": "npm run lint && node ./test.js",
    "lint": "eslint .  && prettier -c \"**/*.js\" \"**/*.json\" \"**/*.md\" \".*rc\" \"**/*.yml\"",
    "fmt": "prettier --write \"**/*.js\" \"**/*.json\" \"**/*.md\" \".*rc\" \"**/*.yml\""
  },
  "devDependencies": {
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "prettier": "^1.19.1"
  }
}
