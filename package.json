{
  "name": "tickplate",
  "version": "1.0.8",
  "author": "Timur Shemsedinov <timur.shemsedinov@gmail.com>",
  "description": "Back-tick templates for JavaScript",
  "license": "MIT",
  "keywords": [
    "tickplate",
    "metarhia",
    "tick",
    "backtick",
    "template",
    "templating"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/metarhia/tickplate.git"
  },
  "bugs": {
    "url": "https://github.com/metarhia/tickplate/issues",
    "email": "timur.shemsedinov@gmail.com"
  },
  "homepage": "https://metarhia.com",
  "funding": {
    "type": "patreon",
    "url": "https://www.patreon.com/tshemsedinov"
  },
  "main": "tickplate.js",
  "types": "tickplate.d.ts",
  "files": [
    "tickplate.d.ts"
  ],
  "engines": {
    "node": "18 || 20 || 21 || 22"
  },
  "readmeFilename": "README.md",
  "scripts": {
    "test": "npm run lint && npm run types && node ./test.js",
    "types": "tsc -p tsconfig.json",
    "lint": "eslint . && prettier -c \"**/*.js\" \"**/*.json\" \"**/*.md\" \".*rc\" \"**/*.yml\"",
    "fmt": "prettier --write \"**/*.js\" \"**/*.json\" \"**/*.md\" \".*rc\" \"**/*.yml\""
  },
  "devDependencies": {
    "eslint": "^8.54.0",
    "eslint-config-metarhia": "^8.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.1.0",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "metautil": "^4.0.1"
  }
}
